datasource DS {
  // optionally set multiple providers
  provider = ["sqlite", "postgresql"]
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model OrgContact {
  id                   Int      @id @default(autoincrement())
  name                 String
  street               String
  suburb               String
  postcode             Int
  coverage             String
  contactName          String
  majorPurpose         String
  contactPN            String
  meetingTime          DateTime
  meetingTimeFrequency String
  meetingPlace         String
  webpage              Boolean
  webpageAddress       String
  newsletter           Boolean
  alpMembers           Boolean
  email                String   @unique
  createdAt            DateTime @default(now())

  git remote add origin https://github.com/DaveGrace/ALPContacts.git
  git push -u origin master
}
